<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ooprpgproto">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ooprpg2icon2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ooprpg2icon2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ooprpgproto">
        <activity
            android:name=".InventoryActivity"
            android:exported="false" />
        <activity
            android:name=".ShopActivity"
            android:exported="false" />
        <activity
            android:name=".BattleActivity"
            android:exported="false" />
        <activity
            android:name=".HubActivity"
            android:exported="false" />
        <activity
            android:name=".NewGameActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>